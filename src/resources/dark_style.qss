QPushButton {
    height: 50px;
    width: 50px;
    max-height: 50px;
    max-width: 50px;
    border-radius: 25px;
    background-color: #333333;
    color: #ffffff;
    border: 2px solid #555555;
    font-size: 16px;
}

QPushButton:hover {
    background-color: #444444;
}

QPushButton:pressed {
    background-color: #555555;
}

QPushButton:disabled {
    background-color: #666666;
    color: #333333;
    border: 2px solid #888888;
}